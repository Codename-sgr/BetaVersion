            android:screenOrientation="nosensor" />
        <activity
            android:name=".BetaVersion"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".AdCategory.otherAds"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".AdCategory.electronics"
            android:screenOrientation="nosensor" />
        <activity
